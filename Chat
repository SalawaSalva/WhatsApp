<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00D1x000000KJBR',
				'TestSalawa',
				'https://talixo--bigtestbox.sandbox.my.site.com/ESWTestSalawa1688115440059',
				{
					scrt2URL: 'https://talixo--bigtestbox.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://talixo--bigtestbox.sandbox.my.site.com/ESWTestSalawa1688115440059/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'></script>
